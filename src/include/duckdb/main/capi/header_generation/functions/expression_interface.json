{
  "group": "expression_interface",
  "deprecated": false,
  "entries": [
    {
      "name": "duckdb_destroy_expression",
      "return_type": "void",
      "params": [
        {
          "type": "duckdb_expression *",
          "name": "expr"
        }
      ],
      "comment": {
        "description": "Destroys the expression and de-allocates its memory.\n\n",
        "param_comments": {
          "expr": "A pointer to the expression."
        }
      }
    },
    {
      "name": "duckdb_expression_return_type",
      "return_type": "duckdb_logical_type",
      "params": [
        {
          "type": "duckdb_expression",
          "name": "expr"
        }
      ],
      "comment": {
        "description": "Returns the return type of an expression.\n\n",
        "param_comments": {
          "expr": "The expression."
        },
        "return_value": "The return type. Must be destroyed with `duckdb_destroy_logical_type`."
      }
    },
    {
      "name": "duckdb_is_constant_expression",
      "return_type": "bool",
      "params": [
        {
          "type": "duckdb_expression",
          "name": "expr"
        }
      ],
      "comment": {
        "description": "Returns whether the expression is a constant expression or not.\n\n",
        "param_comments": {
          "expr": "The expression."
        },
        "return_value": "True, if the expression is a constant expression, else false."
      }
    },
    {
      "name": "duckdb_constant_expression_value",
      "return_type": "duckdb_value",
      "params": [
        {
          "type": "duckdb_expression",
          "name": "expr"
        }
      ],
      "comment": {
        "description": "Returns the value of a constant expression.\n\n",
        "param_comments": {
          "expr": "The expression. The expression class of the expression must be DUCKDB_EXPR_CLASS_CONSTANT."
        },
        "return_value": "The constant value. Must be destroyed with `duckdb_destroy_value`."
      }
    },
    {
      "name": "duckdb_bind_argument_count",
      "return_type": "idx_t",
      "params": [
        {
          "type": "duckdb_bind_info",
          "name": "info"
        }
      ],
      "comment": {
        "description": "Returns the number of input arguments of the scalar function.\n\n",
        "param_comments": {
          "info": "The bind info."
        },
        "return_value": "The number of input arguments."
      }
    },
    {
      "name": "duckdb_bind_argument_at",
      "return_type": "duckdb_expression",
      "params": [
        {
          "type": "duckdb_bind_info",
          "name": "info"
        },
        {
          "type": "idx_t",
          "name": "index"
        }
      ],
      "comment": {
        "description": "Returns the input argument at index of the scalar function.\n\n",
        "param_comments": {
          "info": "The bind info.",
          "index": "The argument index."
        },
        "return_value": "The input argument at index. Must be destroyed with `duckdb_destroy_expression`."
      }
    }
  ]
}